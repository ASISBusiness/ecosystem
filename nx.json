{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "build": {
      "cache": true,
      "dependsOn": [
        "^build"
      ]
    },
    "lint": {
      "cache": true
    },
    "e2e": {
      "cache": true
    }
  },
  "namedInputs": {
    "default": [
      "{projectRoot}/**/*",
      "sharedGlobals"
    ],
    "sharedGlobals": [
      "{workspaceRoot}/.prettierrc.js",
      "{workspaceRoot}/tsconfig.json",
      "{workspaceRoot}/eslintrc.js",
      "{workspaceRoot}/nx.json"
    ],
    "production": [
      "default"
    ]
  }
}
