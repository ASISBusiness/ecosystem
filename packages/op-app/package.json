{
  "name": "op-app",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite -m development",
    "build": "tsc && vite build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.{ts,tsx}\" && pnpm prettier --check \"**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"**/*.{ts,tsx}\" --fix --quiet && pnpm prettier \"**/*.{ts,tsx}\" --write --loglevel=warn",
    "codegen": "ts-node --esm ./scripts/codegen"
  },
  "dependencies": {
    "@eth-optimism/contracts-ts": "^0.17.0",
    "@tanstack/react-query": "^5.10.0",
    "op-viem": "1.1.0",
    "op-wagmi": "^0.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "viem": "1.19.9",
    "wagmi": "2.0.0-beta.5"
  },
  "devDependencies": {
    "@eth-optimism/superchain-registry": "github:ethereum-optimism/superchain-registry",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eta": "^3.1.1",
    "node-fetch": "^3.3.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  }
}
